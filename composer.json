{
    "name": "yafou/clean-architecture-utilities",
    "homepage": "https://github.com/YaFou/clean-architecture-utilities",
    "description": "Powerful tools to create your own business logic",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Foucher",
            "homepage": "https://github.com/YaFou"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "squizlabs/php_codesniffer": "^3.5.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YaFou\\CleanArchitectureUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaFou\\CleanArchitectureUtilities\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true
    }
}
